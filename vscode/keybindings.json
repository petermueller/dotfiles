// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+a alt+left",
    "command": "workbench.action.splitEditorLeft"
  },
  {
    "key": "ctrl+a alt+right",
    "command": "workbench.action.splitEditorRight"
  },
  {
    "key": "ctrl+a alt+down",
    "command": "workbench.action.splitEditorDown"
  },
  {
    "key": "ctrl+a alt+up",
    "command": "workbench.action.splitEditorUp"
  },
  {
    "key": "ctrl+a ctrl+a",
    "command": "cursorLineStart",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+a",
    "command": "-cursorLineStart",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+a shift+left",
    "command": "workbench.action.moveEditorToLeftGroup"
  },
  {
    "key": "ctrl+a shift+right",
    "command": "workbench.action.moveEditorToRightGroup"
  },
  {
    "key": "ctrl+a shift+down",
    "command": "workbench.action.moveEditorToBelowGroup"
  },
  {
    "key": "ctrl+a shift+up",
    "command": "workbench.action.moveEditorToAboveGroup"
  },
  {
    "key": "ctrl+a left",
    "command": "workbench.action.focusLeftGroup"
  },
  {
    "key": "ctrl+a right",
    "command": "workbench.action.focusRightGroup"
  },
  {
    "key": "ctrl+a down",
    "command": "workbench.action.focusBelowGroup"
  },
  {
    "key": "ctrl+a up",
    "command": "workbench.action.focusAboveGroup"
  },
  {
    "key": "ctrl+shift+up",
    "command": "editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+up",
    "command": "-editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+down",
    "command": "editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+down",
    "command": "-editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+down",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+down",
    "command": "-editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+cmd+up",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+up",
    "command": "-editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+w",
    "command": "-workbench.action.closeWindow"
  },
  {
    "key": "shift+cmd+w shift+cmd+w",
    "command": "workbench.action.closeWindow"
  },
  {
    "key": "shift+cmd+u",
    "command": "-workbench.action.output.toggleOutput"
  },
  {
    "key": "shift+cmd+u",
    "command": "cursorRedo"
  },
  {
    "key": "ctrl+a m",
    "command": "editor.action.toggleMinimap"
  },
  {
    "key": "cmd+o",
    "command": "-workbench.action.files.openFile",
    "when": "false"
  },
  {
    "key": "cmd+o",
    "command": "-workbench.action.files.openFileFolder",
    "when": "isMacNative && openFolderWorkspaceSupport"
  },
  {
    "key": "cmd+o",
    "command": "-workbench.action.files.openFolderViaWorkspace",
    "when": "!openFolderWorkspaceSupport && workbenchState == 'workspace'"
  },
  {
    "key": "cmd+o",
    "command": "-workbench.action.files.openLocalFileFolder",
    "when": "remoteFileDialogVisible"
  },
  {
    "key": "cmd+o",
    "command": "workbench.action.output.toggleOutput"
  },
  {
    "key": "cmd+m",
    "command": "continue.focusContinueInput"
  },
  {
    "key": "cmd+l",
    "command": "-continue.focusContinueInput"
  },
  {
    "key": "cmd+m",
    "command": "-markdown.extension.editing.toggleMath",
    "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^markdown$|^rmd$|^quarto$/"
  },
  {
    "key": "alt+tab",
    "command": "editor.action.inlineSuggest.commit",
    "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorHoverFocused && !editorTabMovesFocus && !suggestWidgetVisible"
  },
  {
    "key": "tab",
    "command": "-editor.action.inlineSuggest.commit",
    "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorHoverFocused && !editorTabMovesFocus && !suggestWidgetVisible"
  },
  {
    "key": "shift+cmd+/",
    "command": "workbench.view.extension.continue"
  },
  {
    "key": "alt+cmd+/",
    "command": "-swift.insertFunctionComment",
    "when": "editorTextFocus"
  }
]
